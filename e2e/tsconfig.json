    "jsx": "react-jsx",
    "baseUrl": "./",
    "jsxImportSource": "react",
    "paths": {
      "@*": ["src/*"]
    }
  },
  "include": ["src", "./*.ts", "configs/astro.config.override-components.ts"],
  "exclude": ["node_modules", "dist"]
}
